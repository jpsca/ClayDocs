{#def title="" #}

<theme.Layout title={title} description={meta.get('description', '')}>
  <theme.NavBar />
  <main class="prose m-0 p-0 max-w-full font-sans">
    {{ content }}
  </main>
  <theme.Menu />
  <theme.Footer />
